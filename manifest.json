  {
    "name": "Tab grouping",
    "description" : "Allows the user to create and manage tab groups",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
      "default_popup": "tab_grouping.html",
      "default_icon": "icon.png"
    },
    "icons": {
      "16": "icon.png",
      "32": "icon.png",
      "64": "icon.png",
      "128": "icon.png"
    },
    "background": {
      //"scripts": ["background.js", "actions.js"],
      "scripts": ["background.js"],
      "persistent": false
    },
    "permissions": [
          "storage",
          "tabs"
        ],
    "content_security_policy": "script-src 'self' https://unpkg.com/material-components-web@latest/dist/material-components-web.min.js; object-src 'self'"
  }